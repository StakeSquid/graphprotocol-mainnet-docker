[store]
[store.primary]
connection = "postgresql://${GRAPH_NODE_DB_USER}:${GRAPH_NODE_DB_PASS}@${GRAPH_NODE_DB_HOST}:5432/${GRAPH_NODE_DB_NAME}"
pool_size = 10

[chains]
ingestor = "index_node_0"

[chains.${CHAIN_0_NAME}]
shard = "primary"
provider = [ { label = "${CHAIN_0_NAME}", url = "${CHAIN_0_RPC}", features = ["archive", "traces"] } ]

[deployment]
[[deployment.rule]]
indexers = [ "index_node_0" ]


[general]
query = "query_node_0"
